		{
			"name": "CheckCorrectSorting",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "d5e25375-3f0b-4f58-aae6-c40cc211080a",
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status is ok, response is json\", function () {\r",
							"    pm.response.to.be.ok;\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"pm.test(\"Response time is less than 1500ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Sorting check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var temp_distance = jsonData[jsonData.length - 1].distance;\r",
							"    var temp_time = jsonData[jsonData.length - 1].time;\r",
							"    for (var index = jsonData.length - 2; index >= 0; index--) {\r",
							"        pm.expect(jsonData[index].distance).to.be.within(0, temp_distance);\r",
							"        temp_distance = jsonData[index].distance;\r",
							"        pm.expect(jsonData[index].time).to.be.within(0, temp_time);\r",
							"        temp_time = jsonData[index].time;\r",
							"    }\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
